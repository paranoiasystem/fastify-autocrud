{
    "name": "fastify-autocrud",
    "version": "0.0.4",
    "description": "Plugin for autogenerate CRUD route as fast as possible.",
    "main": "index.js",
    "scripts": {
        "coveralls": "npm run test -- --cov --coverage-report=html && nyc report --reporter=text-lcov | coveralls",
        "lint": "npm run lint:standard",
        "lint:standard": "standard | snazzy",
        "lintfix": "standard --fix",
        "example": "node example/server.js",
        "test": "npm run lint && npm run unit",
        "unit": "tap test/*.test.js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/paranoiasystem/fastify-autocrud.git"
    },
    "keywords": [
        "fastify",
        "CRUD",
        "mongoose"
    ],
    "author": "Marco Ferraioli - @ParanoiaSystem",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paranoiasystem/fastify-autocrud/issues"
    },
    "homepage": "https://github.com/paranoiasystem/fastify-autocrud",
    "dependencies": {
        "boom": "^7.3.0",
        "fastify-plugin": "^1.6.0"
    },
    "devDependencies": {
        "axios": "^0.19.0",
        "coveralls": "^3.0.6",
        "dotenv": "^8.0.0",
        "fastify": "^2.7.1",
        "mongoose": "^8.1.0",
        "mongoose-paginate-v2": "^1.8.0",
        "snazzy": "^8.0.0",
        "standard": "^12.0.0",
        "tap": "^14.6.1"
    }
}
